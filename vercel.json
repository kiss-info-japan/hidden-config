{
    "version": 2,
    "builds": [
      { "src": "api/mistral.js", "use": "@vercel/node" },
      { "src": "public/index.html", "use": "@vercel/static" }
    ],
    "routes": [
      { "src": "/api/mistral", "dest": "api/mistral.js" },
      { "src": "/(.*)", "dest": "/public/$1" }
    ]
  }
  